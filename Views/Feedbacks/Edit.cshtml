@model HallBookingProject.Models.Feedback

@{
    ViewData["Title"] = "Edit";
     Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Edit</h1>

<h4>Feedback</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdFeed" />
              <input type="hidden" asp-for="FullName" />
    
            <div class="form-group">
                <label asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="form-control"    value="Accept" type="submit" />
                  <input asp-for="Email" class="form-control"  value="Reject" type="submit" />
                <span asp-validation-for="Email" class="text-danger"></span>
            </div>

              <input type="hidden" asp-for="Feedback1" />

            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

