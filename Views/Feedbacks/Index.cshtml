@model IEnumerable<HallBookingProject.Models.Feedback>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Testimonial Mange</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Feedback1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Feed)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Feedback1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Feed.Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdFeed">Accept or Reject</a> |
             @*   <a asp-action="Details" asp-route-id="@item.IdFeed">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdFeed">Delete</a>*@
            </td>
        </tr>
}
    </tbody>
</table>
